<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>projection_model</name>
  <version>2.0.0</version>
  <description>ROS2 package for projecting object detections to world coordinates using height models</description>
  
  <maintainer email="mohmedsayed0990@gmail.com">Mohamed Abdelmawgoud</maintainer>
  <license>Apache-2.0</license>
  
  <author email="glawless@example.com">glawless</author>
  
  <buildtool_depend>ament_cmake</buildtool_depend>
  
  <depend>rclcpp</depend>
  <depend>geometry_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>image_geometry</depend>
  <depend>cv_bridge</depend>
  <depend>opencv4</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>pose_cov_ops_interface</depend>
  <depend>rcl_interfaces</depend>
  <build_depend>neural_network_msgs</build_depend>
  <exec_depend>neural_network_msgs</exec_depend> 
  <depend>ros2_utils</depend>


  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  
  <export>
    <build_type>ament_cmake</build_type>
  </export>
